{
  "name": "tempack",
  "description": "Distribution builder",
  "version": "1.0.0",
  "author": {
    "name": "Stanislaw Glogowski",
    "email": "hello@newobject.pl"
  },
  "license": "MIT",
  "bin": "./dist/cmd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stanislaw-glogowski/tempack.git"
  },
  "bugs": {
    "url": "https://github.com/stanislaw-glogowski/tempack/issues"
  },
  "scripts": {
    "prebuild": "rimraf ./dist/*",
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "tslint --exclude node_modules **/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "start:dev": "ts-node ./src/cmd.ts"
  },
  "dependencies": {
    "colors": "^1.3.1",
    "commander": "^2.17.1",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "rimraf": "^2.6.2",
    "jest": "^23.5.0",
    "ts-node": "^7.0.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
